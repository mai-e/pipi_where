<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>pipi_where</display-name>
  
  <context-param>
    <param-name>/login.do</param-name>
    <param-value>jp.co.comnic.lesson.osunegi.pipi_where.controller.LoginAction</param-value>
  </context-param>
  
  <context-param>
  	<param-name>/logout.do</param-name>
  	<param-value>jp.co.comnic.lesson.osunegi.pipi_where.controller.LogoutAction</param-value>
  </context-param>
  
    <context-param>
  	<param-name>/search.do</param-name>
  	<param-value>jp.co.comnic.lesson.osunegi.pipi_where.controller.SearchAction</param-value>
  </context-param>

  <servlet>
    <servlet-name>login</servlet-name>
    <jsp-file>/login.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>map_search</servlet-name>
    <jsp-file>/map_search.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>map_search</servlet-name>
    <url-pattern>/map_search</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>card_list</servlet-name>
    <jsp-file>/card_list.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>card_list</servlet-name>
    <url-pattern>/card_list</url-pattern>
  </servlet-mapping>
</web-app>